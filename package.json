{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight",
  "scripts": {
    "start": "npm-run-all --parallel security-check serve browser-sync sass-watch",
    "security-check": "nsp check",
    "prestart": "babel-node buildScripts/hooks/startMessage.js",
    "serve": "babel-node buildScripts/server-es6.js",
    "share": "lt --port 3000 --subdomain subzerodigital",
    "browser-sync": "browser-sync start --config buildScripts/broswer-sync-config.json",
    "sass": "node-sass --source-map-embed --output src/css src/scss",
    "sass-watch": "node-sass --watch --source-map-embed --output src/css src/scss",
    "tsc-watch": "tsc -w",
    "buildJs": "babel-node buildScripts/webpack-build.js"
  },
  "author": "Stanley Yang",
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.5.6"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "@types/jquery": "^2.0.41",
    "@types/numeral": "0.0.19",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "bootstrap": "^4.0.0-alpha.6",
    "browser-sync": "^2.18.8",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "extract-text-webpack-plugin-webpack-2": "*",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "2.22.0",
    "jasmine": "^2.6.0",
    "jquery": "^3.2.1",
    "jsdom": "9.8.0",
    "jshint": "^2.9.4",
    "jshint-loader": "^0.8.4",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "karma": "^1.7.0",
